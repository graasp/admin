{
  "name": "graasp-compose",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "clsx": "1.1.1",
    "connected-react-router": "6.8.0",
    "history": "5.0.0",
    "i18next": "19.8.4",
    "immutable": "4.0.0-rc.12",
    "js-cookie": "2.2.1",
    "lodash": "4.17.20",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "11.8.4",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-promise": "0.6.0",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.local react-scripts start",
    "build": "react-scripts build",
    "test": "env-cmd -f ./.env.test react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "prettier:check": "prettier --check src/**/*.js",
    "prettier:write": "prettier --write src/**/*.js",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "cypress:open": "cypress open",
    "cypress": "env-cmd -f ./.env.test npm-run-all --parallel start cypress:open",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser chrome"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "cypress": "6.1.0",
    "cypress-react-selector": "2.2.2",
    "env-cmd": "10.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "standard-version": "9.0.0",
    "typescript": "4.1.3"
  }
}
